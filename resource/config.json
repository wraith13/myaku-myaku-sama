    "canonicalUrl": "https://wraith13.github.io/myaku-myaku-sama/",
    "description": "Myaku-Myaku Sama's pattern animation ( this web app is for study )",
    "noscriptMessage": "JavaScript is disabled. Please enable JavaScript.",
    "rendering":
    {
        "marginRate": 0.9
    },
    "styles":
    {
        "regular": {
            "base": "#FFFFFF",
            "main": "#E50012",
            "accent": "#0068B6"
        },
        "monochrome": {
            "base": "#FFFFFF",
            "main": "#000000",
            "accent": "#72716F"
        },
        "splatoon": {
            "base": "#000000",
            "main": "#FD5900",
            "accent": "#0020FE"
        },
        "splatoon2": {
            "base": "#000000",
            "main": "#1AD71A",
            "accent": "#F32C7F"
        },
        "splatoon3": {
            "base": "#000000",
            "main": "#5F3BFE",
            "accent": "#EBFF37"
        }
    },
    "eye":
    {
        "appearRate": 0.12,
        "vanishRate": 0.1,
        "whiteRate": 0.3,
        "irisRate": 0.15
