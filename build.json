{
    "$schema": "https://raw.githubusercontent.com/wraith13/build.js/master/generated/json-schema.json#",
    "modes": {
        "default": {
            "parameters": {
                "__APPLICATION_TITLE__": {
                    "json": "./resource/config.json",
                    "value": "applicationTitle"
                },
                "__BUILD_MESSAGE__": "This file is genereted by build.js. このファイルは build.js によって生成されました。",
                "__BUILD_OPTIONS__": {
                    "call": "command_options"
                },
                "__BUILD_TIMESTAMP__": {
                    "call": "timestamp"
                },
                "__BUILD_TIMESTAMP_TICK__": {
                    "call": "timestamp_tick"
                },
                "__REPOSITORY_URL__": {
                    "json": "./resource/config.json",
                    "value": "repositoryUrl"
                },
                "__CANONICAL_URL__": {
                    "json": "./resource/config.json",
                    "value": "canonicalUrl"
                },
                "__LICENSE__": {
                    "path": "./LICENSE_1_0.txt"
                },
                "__THEME_COLOR__": "black"
            }
        },
        "html": {
            "template": {
                "path": "./index.html.template"
            },
            "output": {
                "path": "./index.html"
            },
            "parameters": {
                "__DESCRIPTION__": {
                    "json": "./resource/config.json",
                    "value": "description"
                },
                "__NOSCRIPT_MESSAGE__": {
                    "json": "./resource/config.json",
                    "value": "noscriptMessage"
                },
                "__EVIL_COMMONJS__": {
                    "path": "./evil-commonjs/index.js",
                    "replace": {
                        "match": "sourceMappingURL=index.js.map",
                        "text": "sourceMappingURL=evil-commonjs/index.js.map"
                    }
                },
                "__EVIL_TIMER__": {
                    "path": "./evil-timer.js/index.js",
                    "replace": {
                        "match": "sourceMappingURL=index.js.map",
                        "text": "sourceMappingURL=evil-timer.js/index.js.map"
                    }
                },
                "__STYLE__": {
                    "path": "./index.css"
                },
                "__SCRIPT__": {
                    "path": "./script/index.js",
                    "replace": {
                        "match": "sourceMappingURL=index.js.map",
                        "text": "sourceMappingURL=script/index.js.map"
                    }
                },
                "__EVIL_COMMONJS_CONFIG__": {
                    "path": "./resource/evil-commonjs.config.json"
                },
                "__EVIL_TIMER_CONFIG__": {
                    "path": "./resource/evil-timer.js.config.json"
                },
                "__PALETTE_ICON___": {
                    "path": "./image/palette.1024.svg",
                    "replace": {
                        "match": "<svg ",
                        "text": "<svg role=\"presentation\" aria-hidden=\"true\" "
                    }
                },
                "__HD_ICON___": {
                    "path": "./image/hd.1024.svg",
                    "replace": {
                        "match": "<svg ",
                        "text": "<svg role=\"presentation\" aria-hidden=\"true\" "
                    }
                },
                "__WATCH_ICON___": {
                    "path": "./image/watch.1024.svg",
                    "replace": {
                        "match": "<svg ",
                        "text": "<svg role=\"presentation\" aria-hidden=\"true\" "
                    }
                },
                "__METER_ICON___": {
                    "path": "./image/meter.1024.svg",
                    "replace": {
                        "match": "<svg ",
                        "text": "<svg role=\"presentation\" aria-hidden=\"true\" "
                    }
                },
                "__FULLSCREEN_ICON__": {
                    "path": "./image/fullscreen.1024.svg",
                    "replace": {
                        "match": "<svg ",
                        "text": "<svg role=\"presentation\" aria-hidden=\"true\" "
                    }
                },
                "__JUMP_OUT_ICON__": {
                    "path": "./image/jump-out.1024.svg",
                    "replace": {
                        "match": "<svg ",
                        "text": "<svg role=\"presentation\" aria-hidden=\"true\" "
                    }
                }
            }
        },
        "script": {
            "processes": [
                "npm run-script \"build script\""
            ]
        },
        "web.manifest": {
            "template": {
                "path": "./web.manifest.template.json"
            },
            "output": {
                "path": "./web.manifest.json"
            },
            "parameters": {
                "__BACKGROUND_COLOR__": "white",
                "__THEME_COLOR__": "#E50012",
                "__DESCRIPTION__": {
                    "json": "./resource/config.json",
                    "value": "description"
                },
                "__LANG__": "en"
            }
        },
        "service-worker": {
            "template": {
                "path": "./sw.js.template"
            },
            "output": {
                "path": "./sw.js"
            },
            "parameters": {
                "__BUILD_TIMESTAMP_YYYYMMDDHHMMSS__": {
                    "call": "timestamp_yyyymmddhhmmss"
                },
                "__ASSETS__": {
                    "path": "./resource/assets.json"
                }
            }
        },
        "all": {
            "steps": [
                {
                    "references": "script"
                },
                {
                    "references": "web.manifest"
                },
                {
                    "references": "service-worker"
                },
                {
                    "references": "html"
                }
            ]
        }
    }
}